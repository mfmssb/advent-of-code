   "source": [
    "f = open(\"data.txt\", \"r\")\n",
    "datastr = f.read()\n",
    "\n",
    "elves = datastr.split(\"\\n\\n\")\n",
    "\n",
    "sacks = []\n",
    "\n",
    "for e in elves:\n",
    "    sack_str = e.split(\"\\n\")\n",
    "    sack_int = [int(x) for x in sack_str]\n",
    "    sacks.append(sum(sack_int))\n",
    "\n",
    "print(max(sacks))"
   ]
  },
